{
  "name": "@nichoth/ssc-flume",
  "description": "SSC db built with [flume db](https://github.com/flumedb/flumedb)",
  "version": "0.0.0",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && concurrently --kill-others \"node example.js\" \"npx ecstatic public\"",
    "build": "npm run build-js",
    "build-js": "browserify -p esmify -t uglifyify --debug browser-example.js > public/bundle.js",
    "build-sass": "sass src/style/main.scss public/style.css",
    "test": "tape test/*.js | tap-spec"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nichoth/ssc-flume.git"
  },
  "author": "Nick Thomas <nichoth@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nichoth/ssc-flume/issues"
  },
  "homepage": "https://github.com/nichoth/ssc-flume#readme",
  "devDependencies": {
    "browserify": "^17.0.0",
    "concurrently": "^6.2.0",
    "ecstatic": "^4.1.4",
    "esmify": "^2.1.1",
    "sass": "^1.34.1",
    "tap-spec": "^5.0.0",
    "tape": "^5.2.2",
    "uglifyify": "^5.0.2"
  },
  "dependencies": {
    "@nichoth/ssc": "^0.0.11",
    "muxrpc": "^6.5.3",
    "pull-ws": "^3.3.2",
    "ssb-backlinks": "^2.1.1",
    "ssb-caps": "^1.1.0",
    "ssb-config": "^3.4.5",
    "ssb-conn": "^5.1.0",
    "ssb-db2": "^2.1.4",
    "ssb-ebt": "^7.0.2",
    "ssb-feed": "^2.3.0",
    "ssb-friends": "^5.0.0",
    "ssb-gossip": "^1.1.1",
    "ssb-keys": "^7.2.2",
    "ssb-master": "^1.0.3",
    "ssb-replicate": "^1.3.3",
    "ssb-server": "^15.3.0"
  }
}
